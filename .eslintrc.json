{
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "plugin:react/recommended","airbnb"],
  "env": {
    "browser": true
  },
  "rules": {
    "no-unused-vars": "warn",
    "import/extensions": "never",
    "no-undef": "warn",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "arrow-body-style": 0,
    "import/no-unresolved": "off",
    "react/forbid-prop-types": ["error", { "forbid": ["any"] }]
  }
}
